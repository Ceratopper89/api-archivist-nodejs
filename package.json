{
  "name": "api-archivist-nodejs",
  "version": "2.0.32",
  "description": "XYO Archivist Serverless API",
  "sideeffects": false,
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "lint": "yarn eslint --ext .js,.jsx,.ts,.tsx ./src",
    "fix": "yarn eslint --ext .js,.jsx,.ts,.tsx ./src --fix",
    "compile": "yarn lint && yarn tsc",
    "watch": "yarn tsc --watch",
    "build": "yarn version --patch --no-git-tag-version && yarn build:ci",
    "build:ci": "yarn lint && yarn compile && yarn serverless package",
    "build:prod": "yarn compile && yarn serverless package --stage prod",
    "start": "yarn compile && yarn serverless offline --httpPort 3030 --useChildProcesses --useWorkerThreads",
    "start:prod": "yarn compile && yarn serverless offline --httpPort 3030 --stage prod --useChildProcesses --useWorkerThreads",
    "clean": "rm -f tsconfig.tsbuildinfo && rm -r -f ./build && rm -r -f ./.serverless && rm -r -f ./.webpack",
    "deploy:sls": "NODE_OPTIONS=--max_old_space_size=8192 yarn serverless deploy --stage beta",
    "deploy:sls:prod": "yarn serverless deploy --stage prod",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install && yarn outdated",
    "license": "yarn license-checker --exclude 'MIT, ISC, Apache-2.0, BSD, BSD-2-Clause, CC-BY-4.0, Unlicense, CC-BY-3.0, CC0-1.0'",
    "tsprune": "yarn ts-prune"
  },
  "author": "Arie Trouw",
  "license": "ISC",
  "dependencies": {
    "@types/elliptic": "^6.4.12",
    "@xyo-network/sdk-xyo-client-js": "^2.1.17",
    "@xyo-network/sdk-xyo-js": "^2.1.22",
    "@xyo-network/sdk-xyo-mongo-js": "^2.1.11",
    "ajv": "^8.6.0",
    "aws-lambda": "^1.0.6",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "http-status-codes": "^2.1.4",
    "lodash": "^4.17.21",
    "mongodb": "^4.0.0-beta.5",
    "noble-ed25519": "^1.2.3",
    "sha.js": "^2.4.11",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.73",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.2",
    "@types/sha.js": "^2.4.0",
    "@types/source-map-support": "0.5.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "aws-sdk": "^2.931.0",
    "elliptic": "^6.5.4",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "fork-ts-checker-webpack-plugin": "^6.2.6",
    "inquirer": "^8.1.1",
    "license-checker": "^25.0.1",
    "prettier": "^2.3.1",
    "serverless": "^2.46.0",
    "serverless-api-gateway-caching": "^1.7.3",
    "serverless-bundle": "^4.3.1",
    "serverless-domain-manager": "^5.1.0",
    "serverless-offline": "^7.0.0",
    "source-map-support": "^0.5.19",
    "ts-loader": "^9.0.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1"
  }
}